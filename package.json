{
  "name": "ionic-app-base",
  "version": "1.4.601",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "rm -rf .angular && rm -rf dist && rm -rf www && rm -rf node_modules/.cache",
    "build": "ng build --configuration production && node appflow-ios-build.js",
    "build:prod": "ng build --configuration production",
    "build:mobile": "ng build --configuration production",
    "android": "npm run build:mobile && npx cap copy android && npx cap open android",
    "build:android": "npm run build:mobile && npx cap sync android",
    "build:android-release": "npm run build:mobile && npx cap sync android && cd android && ./gradlew bundleRelease",
    "set-android-build-number": "node scripts/set-android-build-number.js",
    "ios": "npm run build:mobile && npx cap copy ios && npx cap open ios",
    "sync": "npm run build:mobile && npx cap sync",
    "clean-build": "bash scripts/clean-build.sh",
    "build:ios-local": "ng build --configuration production && node create-ios-platform.js",
    "build:web": "npm run build:prod && node create-web-inject.js",
    "create-web-inject": "node create-web-inject.js",
    "set-ios-build-number": "node scripts/set-ios-build-number.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "e2e:report": "playwright show-report",
    "e2e:codegen": "playwright codegen http://localhost:4200",
    "screenshot": "npx ts-node e2e/capture-screenshot.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/cdk": "^20.0.0",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-browser-dynamic": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@capacitor/android": "^7.4.2",
    "@capacitor/camera": "^7.0.0",
    "@capacitor/cli": "^7.4.2",
    "@capacitor/core": "^7.4.2",
    "@capacitor/filesystem": "^7.1.6",
    "@capacitor/ios": "^7.4.2",
    "@capacitor/live-updates": "^0.4.0",
    "@ionic/angular": "^8.0.0",
    "agentation": "^1.3.2",
    "amazon-cognito-identity-js": "^6.3.16",
    "browser-image-compression": "^2.0.2",
    "chart.js": "^4.5.1",
    "dexie": "^4.2.1",
    "fabric": "^6.9.1",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.12.1",
    "ionicons": "^7.0.0",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.0",
    "ngx-extended-pdf-viewer": "^25.6.0-alpha.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.0",
    "@angular-eslint/builder": "^20.0.0",
    "@angular-eslint/eslint-plugin": "^20.0.0",
    "@angular-eslint/eslint-plugin-template": "^20.0.0",
    "@angular-eslint/schematics": "^20.0.0",
    "@angular-eslint/template-parser": "^20.0.0",
    "@angular/cli": "20.1.6",
    "@angular/compiler-cli": "^20.0.0",
    "@angular/language-service": "^20.0.0",
    "@capacitor/assets": "^3.0.5",
    "@ionic/angular-toolkit": "^12.0.0",
    "@playwright/test": "^1.50.0",
    "@types/fabric": "^5.3.10",
    "@types/jasmine": "~5.1.0",
    "@types/jsdom": "^21.1.7",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.16.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.1",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~5.1.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "sharp": "^0.34.4",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.0"
  }
}
